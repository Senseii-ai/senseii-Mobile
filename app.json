{
  "expo": {
    "name": "SenseiiApp",
    "slug": "SenseiiApp",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "dark",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "permissions": [
        "android.permission.health.READ_STEPS",
        "android.permission.health.READ_FLOORS_CLIMBED",
        "android.permission.health.READ_DISTANCE"
      ],
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.anonymous.SenseiiApp"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "./androidManifestPlugin.js",
      "react-native-health-connect",
      [
        "expo-build-properties",
        {
          "android": {
            "compileSdkVersion": 34,
            "targetSdkVersion": 34,
            "buildToolsVersion": "34.0.0",
            "minSdkVersion": 26
          }
        }
      ],
      [
        "expo-font",
        {
          "fonts": [
            "./assets/fonts/Roboto_Mono/static/RobotoMono-Bold.ttf",
            "./assets/fonts/Roboto_Mono/static/RobotoMono-BoldItalic.ttf",
            "./assets/fonts/Roboto_Mono/static/RobotoMono-Italic.ttf",
            "./assets/fonts/Roboto_Mono/static/RobotoMono-Light.ttf",
            "./assets/fonts/Roboto_Mono/static/RobotoMono-LightItalic.ttf",
            "./assets/fonts/Roboto_Mono/static/RobotoMono-Medium.ttf",
            "./assets/fonts/Roboto_Mono/static/RobotoMono-MediumItalic.ttf",
            "./assets/fonts/Roboto_Mono/static/RobotoMono-Regular.ttf",
            "./assets/fonts/Roboto_Mono/static/RobotoMono-Thin.ttf",
            "./assets/fonts/Roboto_Mono/static/RobotoMono-ThinItalic.ttf",
            "./assets/fonts/Roboto_Mono/static/RobotoMono-SemiBold.ttf"
          ]
        }
      ]
    ]
  }
}
